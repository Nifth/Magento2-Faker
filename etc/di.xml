<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Framework\Console\CommandList">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="nifth_faker_fake" xsi:type="object">Nifth\Faker\Console\Command\Fake</item>
            </argument>
        </arguments>
    </type>

    <type name="Nifth\Faker\Model\FakerProvider">
        <arguments>
            <argument name="fakerList" xsi:type="array">
                <item name="category" xsi:type="object">Nifth\Faker\Model\Faker\Category</item>
                <item name="product" xsi:type="object">Nifth\Faker\Model\Faker\Product</item>
                <item name="customer" xsi:type="object">Nifth\Faker\Model\Faker\Customer</item>
                <item name="customer_address" xsi:type="object">Nifth\Faker\Model\Faker\CustomerAddress</item>
                <item name="order" xsi:type="object">Nifth\Faker\Model\Faker\Order</item>
                <item name="invoice" xsi:type="object">Nifth\Faker\Model\Faker\Invoice</item>
                <item name="shipment" xsi:type="object">Nifth\Faker\Model\Faker\Shipment</item>
            </argument>
        </arguments>
    </type>

    <type name="Nifth\Faker\Model\Faker\Order">
        <arguments>
            <argument name="quoteManagement" xsi:type="object">Magento\Quote\Model\QuoteManagement\Proxy</argument>
        </arguments>
    </type>
</config>
